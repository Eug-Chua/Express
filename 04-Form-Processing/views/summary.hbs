{{#extends "base"}}

{{#block "content"}}
<h1>Thank You.</h1>
<ul>
    <li class="list-group-item">First Name: {{firstName}}</li>
    <li class="list-group-item">Last Name: {{lastName}}</li>
    <li class="list-group-item">Last Name: {{gender}}</li>
    <li class="list-group-item">Hobbies:
        <ul>
            {{#each hobbies}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </li>
    <li class="list-group-item">Country: {{country}}</li>

</ul>
    
{{/block}}

{{/extends}}